#include<iostream>
using namespace std;
class node{
    public:
  int data;
  node* next;
  node(int value){
    data=value;
    next=NULL;
  } 
};
int main(){
    int arr[5]={1,2,3,4,5};
    node* head=NULL;
    node*tail=NULL;
    for(int i=0;i<5;i++){
        if(head==NULL){
            head=new node(arr[i]);
            tail=head;
        }
        else{
            tail->next=new node(arr[i]);
            tail=tail->next;
        }
    }
    if(head!=NULL){
        node* temp=head;
        head=head->next;
        delete temp; 
    }
    node* temp2;
    temp2=head;
    while(temp2){
        cout<<temp2->data<<endl;
        temp2=temp2->next;
    }
}
