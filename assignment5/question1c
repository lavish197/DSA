#include<iostream>
using namespace std;
class node{
    public:
    int data;
    node* next;
    node(int value){
        data=value;
        next=NULL;
    }
};
int main(){
    int x=2;
    int value=9;
    int arr[5]={1,2,3,4,5};
    node* head=NULL;
    node*tail=NULL;
    for(int i=0;i<5;i++){
        if(head==NULL){
            head=new node(arr[i]);
            tail=head;
        }
        else{
            tail->next=new node(arr[i]);
            tail=tail->next;
        }
    }
    x--;
    node* temp=head;
    while(x--){
        temp=temp->next;
    }
    node* temp2=new node(value);
    temp2->next=temp->next;
    temp->next=temp2;
    node* temp3;
    temp3=head;
    while(temp3){
        cout<<temp3->data<<endl;
        temp3=temp3->next;
    }
}
