#include<iostream>
using namespace std;

void concatenate(char s1[], char s2[], char result[]) {
    int i = 0, j = 0;


    while (s1[i] != '\0') {
        result[j++] = s1[i++];
    }

    i=0;
    while (s2[i] != '\0') {
        result[j++] = s2[i++];
    }

    result[j] = '\0'; 
}


int main()
{
char str1[100];
cout << "Enter a string: ";
cin.getline(str1, 100);   
cout << "You entered: " << str1;

char str2[100];
cout << "\nEnter a string: ";
cin.getline(str2, 100);   
cout << "You entered: " << str2;

char concat[200];
concatenate(str1, str2, concat);
cout<<endl<<"Concataneted string is : "<<concat;
} 
