#include<iostream>
using namespace std;

void bubbleSort(int arr[], int n) {
    for(int i = 0; i < n-1; i++) {
        for(int j = 0; j < n-i-1; j++) {
            if(arr[j] > arr[j+1]) {
                
                int temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            }
        }
    }
}

int countDistinct(int arr[], int n) {
    bubbleSort(arr, n);  

    int count = 1; 
    for(int i = 1; i < n; i++) {
        if(arr[i] != arr[i-1]) {
            count++;
        }
    }
    return count;
}

int main() {
    int arr[1000], n;
    cout << "Enter size of array: ";
    cin >> n;

    cout << "Enter the array: ";
    for(int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    cout << "\nNumber of distinct elements are: " << countDistinct(arr, n);
}
