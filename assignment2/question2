#include<iostream>
using namespace std;

int main()
{
    int arr[] = {64,34,25,12,22,11,90};

  
    int temp;

   for(int i = 0; i<7; i++)
    {
        for(int j = 0; j<7; j++)
        {
            if (arr[j] > arr[j+1])
            {
                temp =  arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            }
        }
    }

    cout<<"The Sorted array is : ";
	for(int i = 0 ; i<7; i++)
	{
		cout<<arr[i];
		cout<<" ";
	}
}
