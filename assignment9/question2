void DFS_Stack(int start, int n) {
    for (int i = 0; i < n; i++)
        visited[i] = 0;

    push(start);

    printf("DFS Traversal: ");

    while (top != -1) {
        int node = pop();

        if (!visited[node]) {
            printf("%d ", node);
            visited[node] = 1;
        }
        for (int j = n - 1; j >= 0; j--) {
            if (adj[node][j] == 1 && visited[j] == 0) {
                push(j);
            }
        }
    }
}
